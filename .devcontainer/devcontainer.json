// Devcontainer config inspired by team 2202 https://github.com/2202Programming/FRC2022/blob/main/.devcontainer/devcontainer.json
{
  "name":"FRC Java Development",
  "workspaceMount": "source=${localWorkspaceFolder},target=/root/workspace,type=bind,consistency=cached",
  "workspaceFolder": "/root/workspace",
  "image": "wpilib/roborio-cross-ubuntu:2022-18.04",
  "extensions": [
	"redhat.java",
	"vscjava.vscode-gradle",
	"ms-vscode.cpptools",
	"vscjava.vscode-java-pack",
	"wpilibsuite.vscode-wpilib",
	"richardwillis.vscode-spotless-gradle",
	"2gua.rainbow-brackets",
	"zhuangtongfa.material-theme",
	"aaron-bond.better-comments"
]
}
